
import java.util.*;

public class Factorial {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        int value = sc.nextInt();
        value obj = new value();
        obj.Akash(value);
    }

    void Akash(int n) {
        System.out.println("the value is"+n);
        int fact = 1;
        for (int i =1; i <=n; i++) {
             fact= fact * i;

        }
        System.out.print(fact);
    }
}
